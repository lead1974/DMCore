<!DOCTYPE html>
<html>
<head>
    <meta charset="utf-8">
    <meta name="viewport" content="width=device-width, initial-scale=1.0">
    <meta name="description" content="">
    <meta name="author" content="">
    <title>@ViewData["Title"] | @configuration.GetSection("SiteSettings")["SiteName"]</title>

    <link href="~/css/main.css" rel="stylesheet" />
    <link href="~/lib/bootstrap/dist/css/bootstrap.min.css" rel="stylesheet" />

    <link rel="stylesheet" href="https://kendo.cdn.telerik.com/@configuration.GetSection("VersionSettings")["kendo"]/styles/kendo.common-nova.min.css" />
    <link rel="stylesheet" href="https://kendo.cdn.telerik.com/@configuration.GetSection("VersionSettings")["kendo"]/styles/kendo.nova.min.css" />

    <link href="https://use.fontawesome.com/releases/@configuration.GetSection("VersionSettings")["fontawesome"]/css/all.css" integrity="sha384-UHRtZLI+pbxtHCWp1t77Bi1L4ZtiqrqD80Kn4Z8NTSRyMA2Fd33n5dQ8lWUE00s/" crossorigin="anonymous" rel="stylesheet">
    <link href="~/lib/font-awesome/css/font-awesome.min.css" rel="stylesheet" />
    <link href="~/lib/prettyPhoto/css/prettyPhoto.min.css" rel="stylesheet" />
    <link href="~/lib/animate/animate.min.css" rel="stylesheet" />

    <script src="https://ajax.aspnetcdn.com/ajax/jquery/@configuration.GetSection("VersionSettings")["jQuery"]"
            asp-fallback-src="~/lib/jquery/dist/jquery.min.js"
            asp-fallback-test="window.jQuery"
            crossorigin="anonymous"
            integrity="sha384-tsQFqpEReu7ZLhBV2VZlAu7zcOV+rXbYlF2cqB8txI/8aZajjp4Bqd+V6D5IgvKT">
    </script>
    <script src="~/lib/jquery/dist/jquery.isotope.min.js"></script>
    <script src="~/lib/bootstrap/dist/js/bootstrap.js"></script>

    <script src="https://kendo.cdn.telerik.com/@configuration.GetSection("VersionSettings")["kendo"]/js/jszip.min.js"></script>
    <script src="https://kendo.cdn.telerik.com/@configuration.GetSection("VersionSettings")["kendo"]/js/kendo.all.min.js"></script>
    <script src="https://kendo.cdn.telerik.com/@configuration.GetSection("VersionSettings")["kendo"]/js/kendo.aspnetmvc.min.js"></script>


    <script src="~/lib/prettyPhoto/js/jquery.prettyPhoto.min.js"></script>
    <script src="~/js/html5shiv.js"></script>
    <script src="~/js/main.js" asp-append-version="true"></script>

</head>
<body>
    <nav class="navbar navbar-inverse navbar-fixed-top">
        <div class="container">
            <div class="navbar-header">
                <button type="button" class="navbar-toggle" data-toggle="collapse" data-target=".navbar-collapse">
                    <span class="sr-only">Toggle navigation</span>
                    <span class="icon-bar"></span>
                    <span class="icon-bar"></span>
                    <span class="icon-bar"></span>
                </button>
                <a class="navbar-brand" asp-area="" asp-page="/Default"><img src="~/images/logo.png" alt="logo"></a>
            </div>
            <div class="navbar-collapse collapse">
                Site Administration
                @*<ul class="nav navbar-nav">
                        <li><a asp-page="/Index">Home</a></li>
                        <li><a asp-page="/About">About</a></li>
                        <li><a asp-page="/Contact">Contact</a></li>
                    </ul>*@
            </div>
        </div>
    </nav>
        <section id="breadcrumb" class="container">
            <breadcrumb asp-homepage-title="Site Administration"
                    asp-homepage-url="/admin"
                    asp-bootstrap-version="V4"
                    asp-homepage-glyphicon="glyphicon glyphicon-home"></breadcrumb>
        </section>
        <div class="container body-content">
            @RenderBody()
            <hr />
            <footer id="footer" class="navbar navbar-text">
                <div class="container">
                    <div class="row">
                        <div class="col-sm-12">
                            &copy; @DateTime.Now.Year <a target="_blank" href="http://DealsMo.com/" title="Free Deals Daily and More">@configuration.GetSection("SiteSettings")["SiteDomain"]</a>. All Rights Reserved.
                        </div>
                    </div>
                </div>
            </footer><!--/#footer-->
        </div>


        @RenderSection("Scripts", required: false)
</body>
</html>
